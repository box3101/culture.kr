{
	"editor.wordWrap": "on",
	"editor.fontFamily": "d2coding,verdana",
	"window.zoomLevel": 0.5,
	"editor.mouseWheelZoom": true,
	"editor.lineNumbers": "on",
	"powermode.enabled": false,
	"powermode.maxExplosions": 60,
	"editor.colorDecorators": true,
	"editor.codeLens": false,
	"editor.fontligatures": true,
	"liveServer.settings.donotShowInfoMsg": true,
	"workbench.iconTheme": "vscode-icons",
	"vsicons.dontShowNewVersionMessage": true,
	"liveServer.settings.CustomBrowser": "chrome",
	"liveServer.settings.donotVerifyTags": true,
	"workbench.colorTheme": "Monokai Dimmed",
	"fontAwesomeAutocomplete.triggerWord": "fa-",
	"editor.tabSize": 2,
	"prettier.tabWidth": 2,
	"prettier.useTabs": true,
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[j-vascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"powermode.shake.enabled": false,
	"powermode.explosions.size": 16,
	"powermode.explosions.backgroundMode": "image",
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[scss]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"terminal.integrated.defaultProfile.windows": "Command Prompt",
	"files.autoSave": "onFocusChange",
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"emmet.includeLanguages": {
		"j-vascript": "j-vascriptreact"
	},
	"xml.server.binary.trustedHashes": [
		"007072e03a395315e93fc947e7687e8451f8589b9edd12a58ac70cb1bd3f86b7"
	],
	"git.autofetch": true,
	"turboConsoleLog.includeFileNameAndLineNum": false,
	"turboConsoleLog.insertEnclosingFunction": false,
	"[astro]": {
		"editor.defaultFormatter": "astro-build.astro-vscode"
	}
}
